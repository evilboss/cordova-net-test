<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cloudstaff.evilboss.csnetworktester" version="0.0.5" xmlns="http://www.w3.org/ns/widgets"
        xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>CS-Network-Tester</name>
  <description>
    An app that checks Network Information.
  </description>
  <author email="jr.evilboss@gmail.com" href="https://github.com/evilboss">
    Jr Reyes
  </author>
  <content src="index.html"/>
  <plugin name="cordova-plugin-whitelist" version="1"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <icon src="resources/icons/android/drawable-ldpi/ic_launcher.png" density="ldpi"/>
    <icon src="resources/icons/android/drawable-mdpi/ic_launcher.png" density="mdpi"/>
    <icon src="resources/icons/android/drawable-hdpi/ic_launcher.png" density="hdpi"/>
    <icon src="resources/icons/android/drawable-xhdpi/ic_launcher.png" density="xhdpi"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
</widget>
