<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cloudstaff.evilboss.csnetworktester" version="1.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="CordovaHttpPlugin">
        <param name="android-package" value="com.synconset.CordovaHttpPlugin" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Ping">
        <param name="android-package" value="org.tiste.cordova.ping.Ping" />
    </feature>
    <allow-intent href="market:*" />
    <icon density="ldpi" src="resources/icons/android/drawable-ldpi/ic_launcher.png" />
    <icon density="mdpi" src="resources/icons/android/drawable-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="resources/icons/android/drawable-hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="resources/icons/android/drawable-xhdpi/ic_launcher.png" />
    <name>CS-Network-Tester</name>
    <description>
    An app that checks Network Information.
  </description>
    <author email="jr.evilboss@gmail.com" href="https://github.com/evilboss">
    Jr Reyes
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
